<% widget_bag = VisualWidget.search_widget_bag_for(@website, "d2f5b169c2d15a74518c4083407e6208") %>
<% if !widget_bag.blank? && widget_bag.is_valid? %>
        <aside>
            <div class="container">
                <div class="content_box">
                    <div id="carousel">
                        <ul id="carousel_ul">
    <% widget_bag.photos.each do | photo | %>
                            <li><img src="<%= photo.clip.url(:image) %>"></li>
    <% end %>
                        </ul>
                        <div id="carousel_prev"><i class="fa fa-chevron-circle-left"></i></div>
                        <div id="carousel_next"><i class="fa fa-chevron-circle-right"></i></div>
                    </div>
                </div>
            </div>
        </aside>
<% end %>
