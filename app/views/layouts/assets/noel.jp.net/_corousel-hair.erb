<% slider = VisualWidget.search_widget_bag_for(@website, "1ecfea6bcef8109f8056e5aa38bf0c4d") %>
<% if !slider.blank? && slider.is_valid? %>
<aside id="carousel">
  <div id="carousel-list">
    <ul id="carousel-ul">
    <% slider.photos.each do | photo | %>
      <li><img src="<%= photo.clip.url(:image) %>" alt=""></li>
    <% end %>
    </ul>
  </div>
  <ul id="carousel-nav">
  </ul>
</aside>
<% end %>
