<%  gmapcode = VisualWidget.search_widget_bag_for(@website, "b461d3804244abc3fddc4b5d4a400ce4") %>
<%  if gmapcode.blank? || !gmapcode.is_valid? %>
<%  if @website.gps? %>
<script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=false&key=AIzaSyCvYTVvqH3xpk1ORLQ-Y-d2WWfr1381Hg4"></script>
<script>
google.maps.event.addDomListener(window, 'load', function() {
    var latlng = new google.maps.LatLng(<%= @website.lat %>, <%= @website.lng %>);
    var op = {
        zoom: 17,
        center: latlng,
        mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    var gmap = new google.maps.Map(document.getElementById('gmap'), op);
    var mark = new google.maps.Marker({
        position: latlng,
        title: 'Salon',
        map: gmap
    });
});
</script>
<%  end %>
<%  end %>
