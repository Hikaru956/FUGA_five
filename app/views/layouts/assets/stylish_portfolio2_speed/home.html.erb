<!doctype html>
<% hptitle = VisualWidget.search_widget_bag_for(@website, "30f789413ebbc494fa5a217cc46b64d4") %>
<% fbappid = VisualWidget.search_widget_bag_for(@website, "5bc87a171252a65db055fc461c7ec491") %>
<% og_img  = VisualWidget.search_widget_bag_for(@website, "8b950670a2b241cc9348afa7ac1776e3") %>
<% instafd = VisualWidget.search_widget_bag_for(@website, "c9869f2e27dd708137b976e40e666e8c") %>
<% instaid = VisualWidget.search_widget_bag_for(@website, "4922ee6b245275b44dd375b1efc9b3f1") %>
<% instatk = VisualWidget.search_widget_bag_for(@website, "d2f5b169c2d15a74518c4083407e6208") %>
<html lang="ja">
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#">
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible"    content="IE=edge">
<% if hptitle.blank? || !hptitle.is_valid? %>
<title><%= @website.name %></title>
<% else %>
<title><%= hptitle.data_string %></title>
<% end %>
<% unless @website.wsite_description_shop.blank? %>
<meta name="description"              content="<%= @website.wsite_description_shop %>">
<% end %>
<% unless @website.wsite_keywords.blank? %>
<meta name="keywords"                 content="<%= @website.wsite_keywords %>">
<% end %>
<meta name="viewport"                 content="initial-scale=1">
<meta name="format-detection"         content="telephone=no">
<meta name="format-detection"         content="address=no">
<meta name="application-name"         content="swifty2">
<meta name="Targeted Geographic Area" content="Japan">
<meta name="generator"                content="fuga">
<meta name="copyright"                content="<%= model_name(@website.company) %>">
<% if request.host == 'sr2.swifty.jp' %>
<meta name="robots"                   content="noindex,nofollow">
<% end %>
<% unless fbappid.data_string.blank? %>
<meta property="fb:app_id"            content="<%= fbappid.data_string %>">
<% end %>
<meta property="og:type"              content="website">
<% if request.host == 'sr2.swifty.jp' %>
<meta property="og:url"               content="http://<%= request.host %>/home?wkey=<%= @website.wsite_hash_key %>">
<% else %>
<meta property="og:url"               content="http://<%= request.host %>/home">
<% end %>
<% if hptitle.blank? || !hptitle.is_valid? %>
<meta property="og:title"             content="<%= @website.name %>">
<% else %>
<meta property="og:title"             content="<%= hptitle.data_string %>">
<% end %>
<% if !og_img.blank? && og_img.is_valid? %>
<meta property="og:image"             content="http://<%= request.host %><%= og_img.photos[0].clip.url(:image) %>">
<% end %>
<% unless @website.wsite_description_shop.blank? %>
<meta property="og:description"       content="<%= @website.wsite_description_shop %>">
<% end %>
<meta property="og:site_name"         content="<%= @website.name %>">
<% unless @website.custom_metas.blank? %>
    <%= raw @website.custom_metas %>
<% end %>
<%= auto_discovery_link_tag :rss, "/news_feed.rss", :title => @website.name %>
<%= auto_discovery_link_tag :rss, "/stream_feed.rss", :title => @website.name %>
<%
    @favicon = @website.favicon
    @webclip = @website.apple_touch_icon
%>
<% unless @favicon.blank? %>
<link href="<%= @favicon.clip.url(:image) %>" rel="icon" type="image/vnd.microsoft.icon">
<link href="<%= @favicon.clip.url(:image) %>" rel="shortcut icon" type="image/x-icon">
<% end %>
<% unless @webclip.blank? %>
<link rel="apple-touch-icon" href="<%= @webclip.clip.url(:image) %>">
<% end %>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
<link rel="stylesheet" href="/stylesheets/assets/stylish_portfolio2_speed/style.css">
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/style" %>
</head>
<!-- このサイトはIE9未満では正しく表示されません。 -->
<!--[if lt IE 9]>
<body id="home" class="ie8">
<![endif]-->
<!--[if IE 9]>
<body id="home" class="ie9">
<![endif]-->
<!--[if (gt IE 10)|!(IE)]><!-->
<body id="home">
<!--<![endif]-->
<!-- サイトヘッダー -->
<header id="header">
    <div class="container">
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/header" %>
    </div>
</header>
<nav id="nav">
    <ul>
<!-- ナビゲーション -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/navigation" %>
    </ul>
</nav>
<!-- ロゴパネル背景 -->
<div id="logopanelback">
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/rogopanelback" %>
</div>
<!-- タイポパネル背景 -->
<div id="typoback"></div>
<!-- main -->
<%
    serif = VisualWidget.search_widget_bag_for(@website, "2334d0af16e576431dc71f245a9dfe0f")
    if serif.blank? || !serif.is_valid?
        serif = 0
    else
        serif = serif.data_string.to_i
    end
%>
<%  if serif == 1 %>
<main id="main" class="serif">
<%  else %>
<main id="main">
<%  end %>
    <div class="scroller">
        <!--<![endif]-->
        <!-- このサイトはIE9未満では正しく表示されません。 -->
        <!--[if lt IE 9]>
        <p id="browserupgrade">あなたは<strong>古いブラウザ</strong>をご利用のようです。安心してインターネットをお楽しみいただくために<a href="http://browsehappy.com/">最新のブラウザにアップグレード</a>してください。このサイトは古いブラウザでは正しく表示されません。</p>
        <![endif]-->
<!-- ロゴパネル -->
        <aside id="logopanel">
            <div class="container">
                <div id="logo">
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/rogopanel" %>
                </div>
            </div>
        </aside>
<!-- 最新のNews -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/news_list" %>
<!-- 最新のBLOG -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/blog_list" %>
<!-- 最新のStyle -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/gallery_list" %>
<!-- Google street view -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/street_view" %>
<!-- 店舗説明文と地図 -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/shop_info" %>
<!-- インスタグラム -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/instafeed" %>
<!-- タイポパネル -->
        <aside id="typopanel">
            <div class="container">
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/typopanel" %>
            </div>
        </aside>
<!-- SNS Links -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/sns_links" %>
<!-- フットナビ -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/foot_nav" %>
<!-- サイトフッター -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/footer" %>
    </div>
</main>
<!-- common javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/common_js" %>
<!-- page specific javascript -->
<script src="/javascripts/assets/stylish_portfolio2_speed/home.js"></script>
<!-- social net javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/social_js" %>
<!-- google maps javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/gmaps_js" %>
<!-- Instafeed js -->
<%= render :partial=>"#{LayoutScheme.asset_root}/stylish_portfolio2_speed/instafeed_js" %>
</body>
</html>
