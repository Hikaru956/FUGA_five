    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta name="keywords" content="<%= @website.wsite_keywords %>">
    <%
    	description = @website.wsite_description_shop
    	if @seed.instance_of?(ContentLeaf)
    		description = @seed.description_2	unless 	@seed.description_2.blank?
    	end
    %>
    <meta name="description" content="<%= description %>">
    <meta name="author" content="<%= model_name(@website) %>">

	<% unless @website.custom_metas.blank? %>
		<%= raw @website.custom_metas %>
	<% end %>

	<%
		@favicon = @website.favicon
		@webclip = @website.apple_touch_icon
	%>
	<% unless @favicon.blank? %>
		<link href="<%= @favicon.clip.url() %>" rel="icon" type="image/vnd.microsoft.icon"/>
		<link href="<%= @favicon.clip.url() %>" rel="shortcut icon" type="image/x-icon"/>
	<% end %>
	<% unless @webclip.blank? %>
		<link rel="apple-touch-icon-precomposed" href="<%= @webclip.clip.url() %>">
	<% end %>

    <!-- Bootstrap core CSS -->
    <%= stylesheet_link_tag "/stylesheets/assets/monster/bootstrap-silver.css" %>
    <%= stylesheet_link_tag "/stylesheets/assets/monster/responsive.css" %>

    <!-- Add custom CSS here -->
    <%= stylesheet_link_tag "/stylesheets/assets/monster/style-silver.css" %>
    <%= stylesheet_link_tag "/stylesheets/assets/monster/touchTouch.css" %>
    <%= stylesheet_link_tag "/stylesheets/assets/monster/kwicks-slider.css" %>
    <%= stylesheet_link_tag "/stylesheets/DEMO-TEST.css" %>
 	<link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet">

	<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>


    <!-- Bootstrap core JavaScript -->
    <!-- Placed at the end of the document so the pages load faster -->
    <%= javascript_include_tag "/javascripts/assets/monster/jquery.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/bootstrap.js"%>

    <%= javascript_include_tag "/javascripts/assets/monster/superfish.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/jquery.flexslider-min.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/jquery.kwicks-1.5.1.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/jquery.easing.1.3.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/jquery.cookie.js"%>
    <%= javascript_include_tag "/javascripts/assets/monster/touchTouch.jquery.js"%>

	<script type="text/javascript">if($(window).width()>1024){document.write("<"+"script src='/javascripts/assets/monster/jquery.preloader.js'></"+"script>");}	</script>

	<script>
		 jQuery(window).load(function() {
		 $x = $(window).width();
	if($x > 1024)
	{
	jQuery("#content .row").preloader();    }

     jQuery('.magnifier').touchTouch();
     jQuery('.spinner').animate({'opacity':0},1000,'easeOutCubic',function (){jQuery(this).css('display','none')});
  		  });

	</script>

