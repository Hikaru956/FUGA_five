<!-- SNS Links -->
<%= render :partial=>"#{LayoutScheme.asset_root}/kin-hairstore.com/sns_links" %>
<!-- フットナビ -->
<%= render :partial=>"#{LayoutScheme.asset_root}/kin-hairstore.com/foot_nav" %>
<!-- サイトフッター -->
