<% widget_bag = VisualWidget.search_widget_bag_for(@website, "3f3119b401e143bb5244e6eff2b51d83") %>
<% if !widget_bag.blank? && widget_bag.is_valid? %>
        <aside id="carousel">
            <div class="container">
                <div id="carousel_outer">
                    <div id="carousel_inner">
                        <ul id="carousel_ul">
    <% widget_bag.photos.each do | photo | %>
                            <li>
                                <div class="carousel_back" style="background-image:url('<%= photo.clip.url() %>');">
                                    <img src="<%= photo.clip.url() %>">
                                </div>
                            </li>
    <% end %>
                        </ul>
                        <div id="carousel_prev"><i class="fa fa-chevron-circle-left"></i></div>
                        <div id="carousel_next"><i class="fa fa-chevron-circle-right"></i></div>
                    </div>
                </div>
            </div>
        </aside>
<% end %>
