<% widget_bag = VisualWidget.search_widget_bag_for(@website, "0860b45931657b10e6dd4a75c155b8a5") %>
<% if !widget_bag.blank? && widget_bag.is_valid? %>
        <aside>
            <div class="container">
                <div id="carousel">
                    <ul id="carousel_ul">
<% widget_bag.photos.each do | photo | %>
                        <li><img src="<%= url_for_file_column(photo, :image) %>"></li>
<% end %>
                    </ul>
                    <div id="carousel_prev"><i class="fa fa-chevron-circle-left"></i></div>
                    <div id="carousel_next"><i class="fa fa-chevron-circle-right"></i></div>
                </div>
            </div>
        </aside>
<% end %>
