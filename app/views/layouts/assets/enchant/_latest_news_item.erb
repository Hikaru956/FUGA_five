<% key_photo = key_photo(temp_local) %>
<% unless key_photo.blank? %>
                        <div class="imgbox"><div style="background-image: url('<%= url_for_file_column(key_photo, :image, 'panel') %>')">
                            <a href="<%= url_for(:action=>'news_show', :id=>temp_local, :wkey=>@website.wsite_hash_key) %>">
                                <img src="<%= url_for_file_column(key_photo, :image, 'panel') %>">
                            </a>
<% else %>
                        <div class="imgbox"><div style="background-image: url('//placehold.it/480x360/ffffff/cccccc&amp;text=<%=temp_local.publish_from.strftime('%Y-%m-%d')%>')">
                            <a href="<%= url_for(:action=>'news_show', :id=>temp_local, :wkey=>@website.wsite_hash_key) %>">
                                <img src="//placehold.it/480x360/ffffff/cccccc&amp;text=<%=temp_local.publish_from.strftime('%Y-%m-%d')%>">
                            </a>
<% end %>
                        </div></div>
                        <h2><a href="<%= url_for(:action=>'news_show', :id=>temp_local, :wkey=>@website.wsite_hash_key) %>"><%= model_title(temp_local) %></a></h2>
                        <div class="cat"><i class="fa fa-calendar"></i> <%= temp_local.publish_from.strftime("%Y-%m-%d") %></div>
