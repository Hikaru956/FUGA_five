<!-- SNS Links -->
<%= render :partial=>"#{LayoutScheme.asset_root}/labeau.co.jp/sns_links" %>
<!-- フットナビ -->
<%= render :partial=>"#{LayoutScheme.asset_root}/labeau.co.jp/foot_nav" %>
<!-- サイトフッター -->
