<!-- common javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/labeau.co.jp/common_js" %>
<!-- page specific javascript -->
<script src="/javascripts/assets/labeau.co.jp/home.js"></script>
<!-- social net javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/labeau.co.jp/social_js" %>
<!-- google maps javascript -->
<%= render :partial=>"#{LayoutScheme.asset_root}/labeau.co.jp/gmaps_js" %>
</body>
</html>
