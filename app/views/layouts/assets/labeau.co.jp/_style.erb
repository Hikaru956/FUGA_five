<style>
#header {
<% bg_clr = VisualWidget.search_widget_bag_for(@website, "adb48789c010bbccd79a42346431a988") %>
<% if bg_clr.data_string.blank? %>
    background-color: #424242;
<% else %>
    background-color: <%= bg_clr.data_string %>;
<% end %>
}
#brand a {
<% clr = VisualWidget.search_widget_bag_for(@website, "bc52f57c2304a6e27bb004b15fbf508c") %>
<% if clr.data_string.blank? %>
    color: #fff;
<% else %>
    color: <%= clr.data_string %>;
<% end %>
}
a {
<% style_sheet_uri = (@author_mode) ? @website.wsite_color_edit.repository_path : @website.wsite_color_deploy.repository_path %>
<% case style_sheet_uri %>
<% when 'lavish-bootstrap-0' %>
    color: #304FFE;
<% when 'lavish-bootstrap-1' %>
    color: #d50000;
<% when 'lavish-bootstrap-2' %>
    color: #ff1493;
<% when 'lavish-bootstrap-3' %>
    color: #FFEB3B;
<% when 'lavish-bootstrap-4' %>
    color: #1B5E20;
<% when 'lavish-bootstrap-5' %>
    color: #00C853;
<% when 'lavish-bootstrap-6' %>
    color: #FF6F00;
<% when 'lavish-bootstrap-7' %>
    color: #FFFFFF;
<% when 'lavish-bootstrap-8' %>
    color: #484848;
<% else %>
    color: #ff1493;
<% end %>
}
<% pnl = VisualWidget.search_widget_bag_for(@website, "f077db1c979c64158c04138bf7c64c65") %>
<% if pnl.photos.blank? %>
#logopanelback > div {
    background-image: url('/images/assets/share/w1440h/int001.jpg');
}
<% else %>
    <% pnl.photos.each_with_index do | photo, idx | %>
#logopanelback > div:nth-child(<%= idx+1 %>) {
    background-image: url('<%= photo.clip.url(:image) %>');
}
    <% end %>
<% end %>
<% typ = VisualWidget.search_widget_bag_for(@website, "7e4983251d4fd03c1227fc7f43441f5c") %>
<% if typ.photos.blank? %>
#typoback {
    background-image: url('/images/assets/share/w1440h/int002.jpg');
}
<% else %>
#typoback {
    background-image: url('<%= typ.photos[0].clip.url(:image) %>');
}
<% end %>
</style>
