        <header id="site-header">
            <div class="container">
                <div id="brand">
<% logo_img = VisualWidget.search_widget_bag_for(@website, "81d554684e7a2bab1ec4b2dd040f1e4f") %>
<% if logo_img.blank? || !logo_img.is_valid? %>
                    <a href="/home"><img src="/images/assets/salud-hair.jp/logomark234.png" alt="Logomark"></a>
<% else %>
                    <a href="/home"><img src="<%= url_for_file_column(logo_img.photos[0], :image) %>" alt="Logomark"></a>
<% end %>
                </div>
                <nav id="gNav">
                    <ul>
<%= render :partial=>"#{LayoutScheme.asset_root}/salud-hair.jp/navigation" %>
                    </ul>
                </nav>
            </div>
        </header>
