<html>
	<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/head" %>
	<body class="is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/header" %>

			<!-- Main -->
				<div id="main" class="wrapper style1">
					<div class="container">
						<header class="major">
							<h2><%= @seed.category_label %></h2>
						</header>
						<ul class="actions">
                            <li><a href="/home?wkey=<%= @website.wsite_hash_key %>"><i class="fa fa-home"></i></a></li>
							<li>/<a href="<%= url_for(:action=>'portfolio_index', :id=>@seed.my_type_root_category, :wkey=>@website.wsite_hash_key) %>"><%= model_title(@seed.my_type_root_category) %></a></li>
                            <li>/<%= @seed.category_label %></li>
                        </ul>

						<!-- Table -->
                        <% unless @seed.blank? %>
                            <section>
                                <%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/portfolio_leafs", locals:{cat: @seed} %>
                            </section>
                            <% @seed.children.each do |sub| %>
                                <section>
                                    <%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/portfolio_leafs", locals:{cat: sub} %>
                                </section>
                            <% end %>
                        <% end %>
					</div>
				</div>

			<!-- Footer -->
				<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/footer" %>

		</div>

		<!-- Scripts -->
		<script src="/assets/parallax_one/js/jquery.min.js"></script>
		<script src="/assets/parallax_one/js/jquery.scrolly.min.js"></script>
		<script src="/assets/parallax_one/js/jquery.dropotron.min.js"></script>
		<script src="/assets/parallax_one/js/jquery.scrollex.min.js"></script>
		<script src="/assets/parallax_one/js/browser.min.js"></script>
		<script src="/assets/parallax_one/js/breakpoints.min.js"></script>
		<script src="/assets/parallax_one/js/util.js"></script>
		<script src="/assets/parallax_one/js/main.js"></script>

	</body>
</html>
