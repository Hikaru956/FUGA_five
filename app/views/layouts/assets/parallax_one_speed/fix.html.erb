<html>
	<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/head" %>
	<body class="is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/header" %>

			<!-- Main -->
				<div id="main" class="wrapper style1">
					<div class="container">
						<header class="major">
							<h2><%= model_title(@seed) %></h2>
						</header>
                        <ul class="actions">
                            <li><a href="/home?wkey=<%= @website.wsite_hash_key %>"><i class="fa fa-home"></i></a></li>
                            <li>/<%= model_title(@seed) %></li>
                        </ul>

						<!-- Content -->
							<section id="content">
								<p><% unless @seed.description.blank? %><%= raw @seed.description %><% end %></p>
							</section>

					</div>
				</div>

			<!-- Footer -->
				<%= render :partial=>"#{LayoutScheme.asset_root}/parallax_one_speed/footer" %>

		</div>
		
		<!-- Scripts -->
		<script src="/assets/parallax_one_speed/js/jquery.min.js"></script>
		<script src="/assets/parallax_one_speed/js/jquery.scrolly.min.js"></script>
		<script src="/assets/parallax_one_speed/js/jquery.dropotron.min.js"></script>
		<script src="/assets/parallax_one_speed/js/jquery.scrollex.min.js"></script>
		<script src="/assets/parallax_one_speed/js/browser.min.js"></script>
		<script src="/assets/parallax_one_speed/js/breakpoints.min.js"></script>
		<script src="/assets/parallax_one_speed/js/util.js"></script>
		<script src="/assets/parallax_one_speed/js/main.js"></script>
	</body>
</html>
