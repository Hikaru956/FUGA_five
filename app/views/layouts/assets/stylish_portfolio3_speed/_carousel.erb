<% main_logo = VisualWidget.search_widget_bag_for(@website, "d93ad407654558cbdf23c7c736b3f171") %>
<% bk_slider = VisualWidget.search_widget_bag_for(@website, "812b86bcba65830b9d7f582aac63f6f0") %>
<% if !bk_slider.blank? && bk_slider.is_valid? %>
<ul id="carouselback">
<%   bk_slider.photos.each do | photo | %>
	<li><img src="<%= photo.clip.url(:image) %>" alt=""></li>
<%   end %>
</ul>
<aside id="carousel2">
<% else %>
<aside id="carousel">
<% end %>
<% if !main_logo.blank? && main_logo.is_valid? %>
	<img src="//<%= request.host %><%= main_logo.photos[0].clip.url(:image) %>" alt="logo">
<% end %>
</aside>
