<% logo0 = VisualWidget.search_widget_bag_for(@website, "624fe990084dc4554bd872cf65399c8d") %>
<% logo1 = VisualWidget.search_widget_bag_for(@website, "f56d78a79943b8117eebc08bfdde11fc") %>
<% logo2 = VisualWidget.search_widget_bag_for(@website, "8bda0840801a7b257ed00c1cf52fb41d") %>
<% logo3 = VisualWidget.search_widget_bag_for(@website, "3ceb3cb2775aec3154a77f2eb33ebc68") %>
<footer id="footer">
    <div class="container">
        <div id="salon_logomarks" class="wrapper">
            <div class="col-1p4 text-center">
<% if logo0.photos[0].blank? %>
                <img src="//placehold.it/240x60" alt="logo mark">
<% else %>
                <img src="<%= logo0.photos[0].clip.url() %>" alt="logo mark">
<% end %>
            </div>
            <div class="col-1p4 text-center">
<% if logo1.photos[0].blank? %>
                <img src="//placehold.it/240x60" alt="logo mark">
<% else %>
                <img src="<%= logo1.photos[0].clip.url() %>" alt="logo mark">
<% end %>
            </div>
            <div class="col-1p4 text-center">
<% if logo2.photos[0].blank? %>
                <img src="//placehold.it/240x60" alt="logo mark">
<% else %>
                <img src="<%= logo2.photos[0].clip.url() %>" alt="logo mark">
<% end %>
            </div>
            <div class="col-1p4 text-center">
<% if logo3.photos[0].blank? %>
                <img src="//placehold.it/240x60" alt="logo mark">
<% else %>
                <img src="<%= logo3.photos[0].clip.url() %>" alt="logo mark">
<% end %>
            </div>
        </div>
        <div id="sns_links" class="clearfix">
<% unless @website.wsite_telephone.blank? %>
            <a href="tel:<%= @website.wsite_telephone %>"><i class="fa fa-phone-square"></i></a>
<% end %>
<% unless @website.wsite_email.blank? %>
            <a href="mailto:<%= @website.wsite_email %>"><i class="fa fa-envelope"></i></a>
<% end %>
<% unless @website.social_facebook_uri.blank? %>
            <a href="<%= @website.social_facebook_uri %>" target="_blank"><i class="fa fa-facebook-official"></i></a>
<% end %>
<% unless @website.social_twitter_uri.blank? %>
            <a href="<%= @website.social_twitter_uri %>" target="_blank"><i class="fa fa-twitter-square"></i></a>
<% end %>
<% unless @website.social_gplus_uri.blank? %>
            <a href="<%= @website.social_gplus_uri %>" target="_blank"><i class="fa fa-google-plus-square"></i></a>
<% end %>
<% unless @website.social_instagram_uri.blank? %>
            <a href="<%= @website.social_instagram_uri %>" target="_blank"><i class="fa fa-instagram"></i></a>
<% end %>
<% unless @website.social_line_uri.blank? %>
            <a href="<%= @website.social_line_uri %>" target="_blank"><img src="/images/assets/line_icon.png" alt="LINE" /></a>
<% end %>
<% unless @website.social_pinterest_uri.blank? %>
            <a href="<%= @website.social_pinterest_uri %>" target="_blank"><i class="fa fa-pinterest-square"></i></a>
<% end %>
<% unless @website.social_tumblr_uri.blank? %>
            <a href="<%= @website.social_tumblr_uri %>" target="_blank"><i class="fa fa-tumblr-square"></i></a>
<% end %>
        </div>
        <div id="copyright">
            <small><%= copyright_notice(@website) %></small>
        </div>
    </div>
</footer>
